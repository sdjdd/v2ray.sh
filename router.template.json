{
    "inbounds": [
        {
            "protocol": "socks",
            "listen": "127.0.0.1",
            "port": 1086,
            "settings": {
                "udp": true
            }
        },
        {
            "protocol": "http",
            "listen": "127.0.0.1",
            "port": 1087
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom"
        },
        {
            "protocol": "vmess",
            "settings": {
                "vnext": [{
                    "address": "R_ADDRESS",
                    "port": 443,
                    "users": [{
                        "id": "R_ID",
                        "alterId": 64
                    }]
                }]
            },
            "streamSettings": {
                "network": "ws",
                "security": "tls",
                "wsSettings": {
                    "path": "R_PATH"
                }
            },
            "tag": "proxy"
        }
    ],
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "type": "field",
                "outboundTag": "proxy",
                "domain": [
                    "domain:bit.ly",
                    "domain:dartlang.org",
                    "domain:lang-8.com",
                    "domain:matcha-jp.com",
                    "domain:medium.com",
                    "domain:v2ray.com",
                    "domain:wikipedia.org",
                    "domain:yahoo.co.jp",

                    "geosite:google",
                    "geosite:instagram",
                    "geosite:twitter",
                    "geosite:yahoo"
                ]
            }
        ]
    }
}
